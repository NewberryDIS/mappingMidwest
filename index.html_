    <script>
        var pageId = 'mm'
        $('body').css('background-image','url(\'wp_' + pageId + '-c.jpg\')');
        $('#logo').append(logoText[pageId][0]);
        var contentNavHtml = [];
        for (n in contentNav[pageId]){
